{
  "name": "github-js-sdk",
  "description": "Wrapper for github APIs and types, written in TypeScript",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tinystacks/github-js-sdk.git"
  },
  "keywords": [
    "github",
    "api",
    "sdk"
  ],
  "author": "Zayd Simjee @ tinystacks.com",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/tinystacks/github-js-sdk/issues"
  },
  "homepage": "https://github.com/tinystacks/github-js-sdk#readme",
  "dependencies": {
    "@octokit/core": "^3.3.0",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  }
}
